    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/api",
            "env": {},
            "args": [
                "serve",
            ],
            "windows": {
                "args": [
                    "serve",
                    "--config",
                    "C:\\tmp\\sc-tmp\\api.yml"
                ]
            }
        }
    ]
}
